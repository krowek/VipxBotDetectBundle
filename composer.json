    "type": "symfony-bundle",
    "description": "Symfony VipxBotDetectBundle",
    "keywords": ["Security", "Bot"],
    "homepage": "http://lennerd.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennart Hildebrandt",
            "email": "hello@lennerd.com"
        }
    ],
    "require": {
